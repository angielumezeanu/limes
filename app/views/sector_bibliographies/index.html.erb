<p id="notice"><%= notice %></p>

<h1>Sector Bibliographies</h1>

<table>
  <thead>
    <tr>
      <th>Sector</th>
      <th>Bibliography</th>
      <th>Page</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @sector_bibliographies.each do |sector_bibliography| %>
      <tr>
        <td><%= sector_bibliography.sector %></td>
        <td><%= sector_bibliography.bibliography %></td>
        <td><%= sector_bibliography.page %></td>
        <td><%= link_to 'Show', sector_bibliography %></td>
        <td><%= link_to 'Edit', edit_sector_bibliography_path(sector_bibliography) %></td>
        <td><%= link_to 'Destroy', sector_bibliography, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Sector Bibliography', new_sector_bibliography_path %>
