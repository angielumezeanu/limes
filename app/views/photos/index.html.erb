<p id="notice"><%= notice %></p>

<h1 class="caption">Photos</h1>

<%= link_to 'New Photo', new_photo_path %><br/><br/>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
      <th>Sector</th>
      <th>Imag type</th>
      <th>Description</th>
    </tr>
  </thead>

  <tbody>
    <% @photos.each do |photo| %>
      <tr>
        <td><%= link_to 'Show', photo %></td>
        <td><%= link_to 'Edit', edit_photo_path(photo) %></td>
        <td><%= link_to 'Destroy', photo, method: :delete, data: { confirm: 'Are you sure?' } %></td>
        <td><% unless photo.sector.blank? %><%=h photo.sector.code %> <% end %></td>
        <td><%= photo.imag_type %></td>
        <td><%= photo.description %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Photo', new_photo_path %>
