<p id="notice"><%= notice %></p>

<h1>Wholes</h1>

<table>
  <thead>
    <tr>
      <th>Degree</th>
      <th>Description</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @wholes.each do |whole| %>
      <tr>
        <td><%= whole.degree %></td>
        <td><%= whole.description %></td>
        <td><%= link_to 'Show', whole %></td>
        <td><%= link_to 'Edit', edit_whole_path(whole) %></td>
        <td><%= link_to 'Destroy', whole, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Whole', new_whole_path %>
